{
  "root": "libs/bolt",
  "sourceRoot": "libs/bolt/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/bolt/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/bolt"],
      "options": {
        "jestConfig": "libs/bolt/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
